{
  "name": "webman/validation",
  "type": "library",
  "license": "MIT",
  "description": "Webman plugin webman/validation",
  "require": {
    "illuminate/validation": "*",
    "illuminate/translation": "*",
    "workerman/webman-framework": "^2.1 || dev-master",
    "webman/console": ">=2.1.7"
  },
  "autoload": {
    "psr-4": {
